<!DOCTYPE html>
<html>
	<head>
		<title>Ledger</title>
		<link rel="stylesheet" href="/public/bootstrap-icons/font/bootstrap-icons.css">
		<link rel="stylesheet" href="/style/style.css">
		<script defer src="/dist/common.js"></script>
		<base href="/" />
	</head>
	<body class="layout-fixed" id="body"
			term={{term}}
			user={{user}}>
		<div class="wrapper  d-flex flex-column h-100">
    		<nav class="main-header navbar navbar-expand-lg navbar-light bg-light p-3"
				id="common-nav"></nav>
	    	<aside 
				class="main-sidebar sidebar-bg-dark sidebar-color-primary shadow"
				id="sidebar"></aside>
       		<main class="content-wrapper overflow-y-auto h-100">
				<div class="content">
					<div class="container" id="ledger">
					</div>
				</div>
      		</main>
			<footer
				class="main-footer"
				id="common-footer"></footer>
		</div>
		<script defer src="/dist/ledger.js"></script>
	</body>
</html>
